    "cypress-split": "1.23.1",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-cypress": "3.0.2",
    "mocha": "10.4.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "typescript": "5.4.5"
