    "cypress-split": "1.24.0",
    "dotenv": "16.4.5",
    "eslint": "9.9.0",
    "eslint-plugin-cypress": "3.5.0",
    "mocha": "10.7.3",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "typescript": "5.5.4"
