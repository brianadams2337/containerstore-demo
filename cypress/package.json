{
  "name": "@scayle/storefront-boilerplate-nuxt-cypress",
  "description": "Cypress E2E Testing suite for SCAYLE Storefront Boilerplate",
  "private": true,
  "engines": {
    "node": ">= 20.7.0"
  },
  "scripts": {
