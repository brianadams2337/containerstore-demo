{
  "name": "@scayle/storefront-boilerplate-nuxt",
  "description": "SCAYLE Storefront demo shop application based on Nuxt 3",
  "version": "1.7.0",
  "private": true,
  "workspaces": [
    "./"
  ],
  "engines": {
    "node": ">= 20.7.0"
  },
  "website": "https://scayle.dev/en/storefront-guide/developer-guide/getting-started/about-storefront",
  "type": "module",
  "scripts": {
    "assert-mandatory-env-vars": "echo 'skipped'",
    "build": "nuxt build",
    "dev": "nuxt dev --development",
    "dev:https": "nuxt dev --https",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "patch-package && nuxt prepare",
    "lint:fix": "eslint . --fix",
    "lint:script": "eslint .",
    "lint": "yarn lint:script",
    "typecheck": "nuxi typecheck",
    "format": "prettier --ignore-unknown --log-level warn --check .",
    "format:fix": "prettier --ignore-unknown --log-level warn --write .",
    "e2e:ui": "yarn --cwd ./playwright e2e:ui",
    "e2e:test": "yarn --cwd ./playwright e2e:test",
    "start:node": "node --env-file .env .output/server/index.mjs",
    "storyblok:download": "env-cmd -f .env.storyblok -x storyblok pull-components --space \\$STORYBLOK_SPACE_ID",
    "storyblok:generate": "node modules/cms/providers/storyblok/scripts/storyblok-generate.cjs && prettier --write modules/cms/providers/storyblok/types/storyblok.gen.d.ts",
    "storyblok:login": "env-cmd -f .env.storyblok -x storyblok login --token \\$STORYBLOK_PERSONAL_TOKEN --region eu",
    "storyblok:unused": "node modules/cms/providers/storyblok/scripts/storyblok-unused.cjs",
    "contentful:download": "env-cmd -f .env.contentful -x contentful-export --space-id \\$CONTENTFUL_SPACE_ID --management-token \\$CONTENTFUL_MANAGEMENT_TOKEN  --skip-webhooks --skip-roles --content-file contentful.\\$CONTENTFUL_SPACE_ID.json",
    "contentful:generate": "env-cmd -f .env.contentful -x cf-content-types-generator -X --response --typeguard ./contentful.\\$CONTENTFUL_SPACE_ID.json --out ./modules/cms/providers/contentful/types/gen && prettier --write modules/cms/providers/contentful/types/gen/** && find ./modules/cms/providers/contentful/types/gen -type f -name '*.ts' -print0 | xargs -0 sed -i '' -E \"s/Type([[:alnum:]]+)<'WITHOUT_(LINK_RESOLUTION|UNRESOLVABLE_LINKS)'>/Type\\1<'WITHOUT_\\2', LocaleCode>/g\"  ",
    "test": "vitest",
    "test:ci": "vitest --run --coverage --reporter=default --reporter=junit --outputFile=./coverage/junit.xml",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.12",
    "@eslint/eslintrc": "3.2.0",
    "@gtm-support/vue-gtm": "3.1.0",
    "@nuxt/eslint": "1.0.0",
    "@nuxt/image": "1.9.0",
    "@nuxt/test-utils": "3.15.4",
    "@nuxtjs/i18n": "9.1.1",
    "@nuxtjs/tailwindcss": "6.13.1",
    "@scayle/eslint-config-storefront": "4.4.1",
    "@scayle/eslint-plugin-vue-composable": "0.2.1",
    "@scayle/prettier-config-storefront": "2.0.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/vue": "8.1.0",
    "@types/color": "4.2.0",
    "@types/node": "22.13.1",
    "@typescript-eslint/scope-manager": "8.23.0",
    "@typescript-eslint/utils": "8.23.0",
    "@upstash/redis": "1.34.4",
    "@vitest/coverage-v8": "2.1.9",
    "@vue/test-utils": "2.4.6",
    "@vue/typescript-plugin": "2.2.0",
    "autoprefixer": "10.4.20",
    "env-cmd": "10.1.0",
    "eslint": "9.19.0",
    "eslint-formatter-gitlab": "5.1.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-unimport": "0.1.2",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "fishery": "2.2.3",
    "happy-dom": "17.0.0",
    "jiti": "2.4.2",
    "lint-staged": "15.4.3",
    "nuxt": "3.14.1592",
    "nuxt-svgo": "4.0.14",
    "ofetch": "1.4.1",
    "patch-package": "8.0.0",
    "pathe": "2.0.2",
    "postcss": "8.5.1",
    "postcss-custom-properties": "14.0.4",
    "postcss-html": "1.8.0",
    "postcss-import": "16.1.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.0.3",
    "storyblok": "3.35.2",
    "storyblok-generate-ts": "2.2.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "unimport": "4.0.0",
    "utility-types": "3.11.0",
    "vitest": "2.1.9",
    "vitest-environment-nuxt": "1.0.1",
    "vue-tsc": "2.2.0"
  },
  "dependencies": {
    "@contentful/live-preview": "4.6.5",
    "@contentful/rich-text-html-renderer": "17.0.0",
    "@googlemaps/js-api-loader": "1.16.8",
    "@nuxt/fonts": "0.10.3",
    "@scayle/nuxt-opentelemetry": "0.5.7",
    "@scayle/omnichannel-nuxt": "4.0.4",
    "@scayle/storefront-country-detection": "1.1.0",
    "@scayle/storefront-nuxt": "8.7.0",
    "@scayle/storefront-product-detail": "1.0.2",
    "@scayle/storefront-product-listing": "1.1.3",
    "@storyblok/nuxt": "6.2.2",
    "@storyblok/richtext": "3.0.2",
    "@storyblok/vue": "8.1.10",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@types/google.maps": "3.58.1",
    "@vercel/kv": "3.0.0",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/components": "12.5.0",
    "@vueuse/core": "12.5.0",
    "@vueuse/gesture": "2.0.0",
    "@vueuse/integrations": "12.5.0",
    "@vueuse/nuxt": "12.5.0",
    "axios": "1.7.9",
    "cf-content-types-generator": "2.15.5",
    "check-password-strength": "3.0.0",
    "citty": "0.1.6",
    "color": "4.2.3",
    "consola": "3.4.0",
    "contentful": "11.4.5",
    "contentful-export": "7.21.22",
    "dompurify": "3.2.4",
    "dotenv": "16.4.7",
    "focus-trap": "7.6.4",
    "global": "4.4.0",
    "globby": "14.0.2",
    "knitwork": "1.2.0",
    "maska": "3.1.0",
    "nanoid": "5.0.9",
    "nuxi": "3.21.1",
    "nuxt-swiper": "1.2.2",
    "redis": "4.7.0",
    "schema-dts": "1.1.2",
    "storyblok-js-client": "6.10.7",
    "tabbable": "6.2.0",
    "ufo": "1.5.4",
    "vue": "3.5.13",
    "vue-slider-component": "4.1.0-beta.7"
  },
  "lint-staged": {
    "./**/*{.js,.mjs,.cjs,.ts,.vue}\"": [
      "prettier --write ",
      "eslint --fix "
    ],
    "./**/*{.md,.json,.css}\"": "prettier --write "
  },
  "volta": {
    "node": "22.13.1"
  },
  "resolutions": {
    "import-in-the-middle": "1.12.0",
    "@nuxt/kit": "3.14.1592",
    "@nuxt/schema": "3.14.1592",
    "vue": "3.5.13",
    "@vue/compiler-dom": "3.5.13",
    "@vue/compiler-sfc": "3.5.13",
    "@vue/compiler-core": "3.5.13",
    "@vue/compiler-ssr": "3.5.13",
    "@vue/shared": "3.5.13",
    "globals": "15.14.0"
  }
}
