{
  "name": "@scayle/storefront-boilerplate-nuxt",
  "description": "SCAYLE demo shop application based on Nuxt 3",
  "version": "1.0.0-rc.01",
  "private": true,
  "engines": {
    "node": ">= 18.17.1"
  },
  "scripts": {
    "assert-mandatory-env-vars": "echo 'skipped'",
    "build": "nuxt build",
    "dev": "nuxt dev --development",
    "dev:https": "nuxt dev --https",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:fix": "yarn lint:script --fix && yarn lint:style --fix && yarn typecheck",
    "lint:script": "eslint --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint": "yarn lint:script && yarn lint:style && yarn format && yarn typecheck",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --ignore-unknown --log-level warn --write .",
    "format:fix": "prettier --ignore-unknown --log-level warn --write .",
    "cy:open": "cypress open",
    "cy:run:mobile": "cypress run --env mobile=true",
    "cy:run": "cypress run",
    "start:node": "node .output/server/index.mjs",
    "storyblok:download": "env-cmd -f .env.storyblok -x storyblok pull-components --space \\$STORYBLOK_SPACE_ID",
    "storyblok:generate": "node scripts/storyblok-generate.cjs",
    "storyblok:login": "env-cmd -f .env.storyblok -x storyblok login --token \\$STORYBLOK_PERSONAL_TOKEN --region eu",
    "storyblok:unused": "node scripts/storyblok-unused.cjs"
  },
  "devDependencies": {
    "@aboutyou/storyblok-generate-ts": "1.14.0",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "1.0.0-rc.3",
    "@nuxtjs/i18n": "8.0.0-rc.5",
    "@nuxtjs/tailwindcss": "6.8.0",
    "@scayle/eslint-config-storefront": "3.2.3",
    "@scayle/prettier-config-storefront": "2.0.1",
    "@types/color": "3.0.4",
    "@types/node": "18.18.3",
    "@zadigetvoltaire/nuxt-gtm": "0.0.13",
    "autoprefixer": "10.4.16",
    "cypress": "13.3.0",
    "cypress-real-events": "1.10.3",
    "env-cmd": "10.1.0",
    "eslint": "8.50.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "nuxt": "3.7.3",
    "nuxt-svgo": "3.5.4",
    "nuxt-viewport": "2.0.6",
    "ofetch": "1.3.3",
    "postcss": "8.4.31",
    "postcss-custom-properties": "13.3.2",
    "postcss-html": "1.5.0",
    "postcss-import": "15.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.5",
    "sass": "1.68.0",
    "storyblok": "3.25.2",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-config-standard-vue": "1.0.0",
    "stylelint-high-performance-animation": "1.9.0",
    "tailwindcss": "3.3.3",
    "utility-types": "3.10.0",
    "vue-tsc": "1.8.15"
  },
  "dependencies": {
    "@scayle/storefront-nuxt": "7.26.0",
    "@storyblok/nuxt": "5.7.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.4.1",
    "@vueuse/nuxt": "10.4.1",
    "check-password-strength": "2.0.7",
    "color": "4.2.3",
    "dotenv": "16.3.1",
    "envalid": "7.3.1",
    "maska": "2.1.10",
    "nanoid": "5.0.1",
    "nuxt-jsonld": "2.0.8",
    "nuxt-lazy-hydrate": "1.0.0",
    "nuxt-swiper": "1.2.2",
    "pathe": "1.1.1",
    "radash-nuxt": "1.0.2",
    "redis": "4.6.10",
    "schema-dts": "1.1.2",
    "storyblok-js-client": "6.2.0",
    "ufo": "1.3.1",
    "vue-slider-component": "4.1.0-beta.7",
    "yn": "5.0.0"
  },
  "volta": {
    "node": "20.8.0"
  }
}
