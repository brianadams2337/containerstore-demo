{
  "name": "@scayle/storefront-boilerplate-nuxt",
  "description": "SCAYLE demo shop application based on Nuxt 3",
  "version": "1.0.0-rc.04",
  "private": true,
  "engines": {
    "node": ">= 20.7.0"
  },
  "scripts": {
    "assert-mandatory-env-vars": "echo 'skipped'",
    "build": "nuxt build",
    "dev": "nuxt dev --development",
    "dev:https": "nuxt dev --https",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:fix": "yarn lint:script --fix && yarn lint:style --fix",
    "lint:script": "eslint --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint": "yarn lint:script && yarn lint:style",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --ignore-unknown --log-level warn --check .",
    "format:fix": "prettier --ignore-unknown --log-level warn --write .",
    "cy:open": "cypress open",
    "cy:run:mobile": "cypress run --env mobile=true",
    "cy:run": "cypress run",
    "start:node": "node .output/server/index.mjs",
    "storyblok:download": "env-cmd -f .env.storyblok -x storyblok pull-components --space \\$STORYBLOK_SPACE_ID",
    "storyblok:generate": "node scripts/storyblok-generate.cjs && prettier --write cms/storyblok/types/storyblok.gen.d.ts",
    "storyblok:login": "env-cmd -f .env.storyblok -x storyblok login --token \\$STORYBLOK_PERSONAL_TOKEN --region eu",
    "storyblok:unused": "node scripts/storyblok-unused.cjs",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@crowdin/cli": "3.15.0",
    "@nuxt/image": "1.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.11",
    "@nuxtjs/tailwindcss": "6.9.5",
    "@scayle/eslint-config-storefront": "3.2.6",
    "@scayle/prettier-config-storefront": "2.0.2",
    "@types/color": "3.0.6",
    "@types/node": "20.10.4",
    "@vitest/coverage-v8": "0.34.6",
    "@zadigetvoltaire/nuxt-gtm": "0.0.13",
    "autoprefixer": "10.4.16",
    "cypress": "13.6.1",
    "cypress-real-events": "1.11.0",
    "env-cmd": "10.1.0",
    "eslint": "8.55.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "happy-dom": "12.10.3",
    "lint-staged": "15.2.0",
    "nuxt": "3.8.2",
    "nuxt-svgo": "3.7.0",
    "nuxt-viewport": "2.0.6",
    "nuxt-vitest": "0.11.5",
    "ofetch": "1.3.3",
    "postcss": "8.4.32",
    "postcss-custom-properties": "13.3.2",
    "postcss-html": "1.5.0",
    "postcss-import": "15.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "storyblok": "3.25.3",
    "storyblok-generate-ts": "2.0.1",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "12.0.0",
    "stylelint-config-standard-vue": "1.0.0",
    "stylelint-high-performance-animation": "1.9.0",
    "tailwindcss": "3.3.6",
    "typescript": "5.3.3",
    "utility-types": "3.10.0",
    "vitest": "0.33.0",
    "vue-tsc": "1.8.25"
  },
  "dependencies": {
    "@scayle/storefront-nuxt": "7.44.0",
    "@storyblok/nuxt": "5.7.10",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.7.0",
    "@vueuse/nuxt": "10.7.0",
    "check-password-strength": "2.0.7",
    "color": "4.2.3",
    "dotenv": "16.3.1",
    "global": "4.4.0",
    "maska": "2.1.10",
    "nanoid": "5.0.4",
    "nuxi": "3.10.0",
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.2",
    "radash-nuxt": "1.0.2",
    "redis": "4.6.11",
    "schema-dts": "1.1.2",
    "storyblok-js-client": "6.4.0",
    "ufo": "1.3.2",
    "vue-slider-component": "4.1.0-beta.7",
    "yn": "5.0.0"
  },
  "lint-staged": {
    "./**/*{.js,.ts,.vue}\"": [
      "prettier --write ",
      "eslint --fix "
    ],
    "./**/*{.md,.json,.css}\"": "prettier --write "
  },
  "volta": {
    "node": "20.10.0"
  }
}
