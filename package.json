{
  "name": "@scayle/storefront-boilerplate-nuxt",
  "description": "SCAYLE Storefront demo shop application based on Nuxt 3",
  "version": "1.0.0-rc.8",
  "private": true,
  "engines": {
    "node": ">= 20.7.0"
  },
  "type": "module",
  "workspaces": [
    "./"
  ],
  "scripts": {
    "assert-mandatory-env-vars": "echo 'skipped'",
    "build": "nuxt build",
    "dev": "nuxt dev --development",
    "dev:https": "nuxt dev --https",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "patch-package && nuxt prepare",
    "lint:fix": "yarn lint:script --fix",
    "lint:script": "eslint --ignore-path .gitignore .",
    "lint": "yarn lint:script",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --ignore-unknown --log-level warn --check .",
    "format:fix": "prettier --ignore-unknown --log-level warn --write .",
    "cy:open": "yarn --cwd ./cypress cy:open",
    "cy:run:mobile": "yarn --cwd ./cypress cy:run:mobile",
    "cy:run": "yarn --cwd ./cypress cy:run",
    "start:node": "node .output/server/index.mjs",
    "storyblok:download": "env-cmd -f .env.storyblok -x storyblok pull-components --space \\$STORYBLOK_SPACE_ID",
    "storyblok:generate": "node modules/cms/providers/storyblok/scripts/storyblok-generate.cjs && prettier --write modules/cms/providers/storyblok/types/storyblok.gen.d.ts",
    "storyblok:login": "env-cmd -f .env.storyblok -x storyblok login --token \\$STORYBLOK_PERSONAL_TOKEN --region eu",
    "storyblok:unused": "node modules/cms/providers/storyblok/scripts/storyblok-unused.cjs",
    "contentful:download": "env-cmd -f .env.contentful -x contentful-export --space-id \\$CONTENTFUL_SPACE_ID --management-token \\$CONTENTFUL_MANAGEMENT_TOKEN  --skip-webhooks --skip-roles --content-file contentful.\\$CONTENTFUL_SPACE_ID.json",
    "contentful:generate": "env-cmd -f .env.contentful -x cf-content-types-generator -X --response --typeguard ./contentful.\\$CONTENTFUL_SPACE_ID.json --out ./modules/cms/providers/contentful/types/gen && prettier --write modules/cms/providers/contentful/types/gen/** && find ./modules/cms/providers/contentful/types/gen -type f -name '*.ts' -print0 | xargs -0 sed -i '' -E \"s/Type([[:alnum:]]+)<'WITHOUT_(LINK_RESOLUTION|UNRESOLVABLE_LINKS)'>/Type\\1<'WITHOUT_\\2', LocaleCode>/g\"  ",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@crowdin/cli": "3.19.2",
    "@nuxt/image": "1.3.0",
    "@nuxt/test-utils": "3.12.0",
    "@nuxtjs/i18n": "8.3.0",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@scayle/eslint-config-storefront": "3.2.6",
    "@scayle/eslint-plugin-vue-composable": "0.1.1",
    "@scayle/prettier-config-storefront": "2.0.2",
    "@types/color": "3.0.6",
    "@types/node": "20.12.4",
    "@upstash/redis": "1.29.0",
    "@vitest/coverage-v8": "1.4.0",
    "@zadigetvoltaire/nuxt-gtm": "0.0.13",
    "autoprefixer": "10.4.19",
    "env-cmd": "10.1.0",
    "eslint": "8.57.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-tailwindcss": "3.15.1",
    "lint-staged": "15.2.2",
    "nuxt": "3.11.1",
    "nuxt-svgo": "4.0.0",
    "ofetch": "1.3.4",
    "patch-package": "8.0.0",
    "postcss": "8.4.38",
    "postcss-custom-properties": "13.3.6",
    "postcss-html": "1.6.0",
    "postcss-import": "16.1.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.13",
    "storyblok": "3.30.0",
    "storyblok-generate-ts": "2.0.2",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.4",
    "utility-types": "3.11.0",
    "vitest": "1.4.0",
    "vitest-environment-nuxt": "1.0.0",
    "vue-tsc": "2.0.7"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "16.3.5",
    "@googlemaps/js-api-loader": "1.16.6",
    "@scayle/omnichannel-nuxt": "2.1.3",
    "@scayle/storefront-nuxt": "7.62.4",
    "@storyblok/nuxt": "6.0.6",
    "@storyblok/vue": "8.0.7",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.12",
    "@types/google.maps": "3.55.6",
    "@vercel/kv": "1.0.1",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.9.0",
    "@vueuse/nuxt": "10.9.0",
    "axios": "1.6.8",
    "cf-content-types-generator": "2.15.0",
    "check-password-strength": "2.0.10",
    "citty": "0.1.6",
    "color": "4.2.3",
    "consola": "3.2.3",
    "contentful": "10.6.21",
    "contentful-export": "7.19.125",
    "dotenv": "16.4.5",
    "global": "4.4.0",
    "globby": "14.0.1",
    "maska": "2.1.11",
    "nanoid": "5.0.6",
    "nuxi": "3.11.1",
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.2",
    "radash-nuxt": "1.0.2",
    "redis": "4.6.13",
    "schema-dts": "1.1.2",
    "storyblok-js-client": "6.7.1",
    "ufo": "1.5.3",
    "vue": "3.4.21",
    "vue-slider-component": "4.1.0-beta.7",
    "yn": "5.0.0"
  },
  "lint-staged": {
    "./**/*{.js,.mjs,.cjs,.ts,.vue}\"": [
      "prettier --write ",
      "eslint --fix "
    ],
    "./**/*{.md,.json,.css}\"": "prettier --write "
  },
  "volta": {
    "node": "20.12.0"
  }
}
