{
  "name": "nuxt-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:fix": "yarn lint:script --fix && yarn lint:style --fix && yarn typecheck",
    "lint:script": "eslint --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint": "yarn lint:script && yarn lint:style && yarn format && yarn typecheck",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --ignore-unknown --loglevel warn --write .",
    "format:fix": "prettier --ignore-unknown --log-level warn --write ."
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxtjs/i18n": "^8.0.0-rc.3",
    "@scayle/eslint-config-storefront": "^3.2.3",
    "@scayle/prettier-config-storefront": "^2.0.1",
    "@types/node": "^18.17.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "nuxt": "^3.6.5",
    "nuxt-svgo": "^3.4.0",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-high-performance-animation": "^1.9.0",
    "tailwindcss": "^3.3.3",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vueuse/nuxt": "^10.3.0",
    "nuxt-jsonld": "^2.0.8",
    "nuxt-lazy-hydrate": "^1.0.0",
    "ufo": "^1.2.0"
  }
}
