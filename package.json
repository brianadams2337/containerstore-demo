{
  "name": "@scayle/storefront-boilerplate-nuxt",
  "description": "SCAYLE Storefront demo shop application based on Nuxt 3",
  "version": "1.2.0",
  "private": true,
  "engines": {
    "node": ">= 20.7.0"
  },
  "type": "module",
  "workspaces": [
    "./"
  ],
  "scripts": {
    "assert-mandatory-env-vars": "echo 'skipped'",
    "build": "nuxt build",
    "dev": "nuxt dev --development",
    "dev:https": "nuxt dev --https",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "patch-package && nuxt prepare",
    "lint:fix": "eslint . --fix",
    "lint:script": "eslint .",
    "lint": "yarn lint:script",
    "typecheck": "nuxi typecheck",
    "format": "prettier --ignore-unknown --log-level warn --check .",
    "format:fix": "prettier --ignore-unknown --log-level warn --write .",
    "e2e:ui": "yarn --cwd ./playwright e2e:ui",
    "e2e:test": "yarn --cwd ./playwright e2e:test",
    "start:node": "node --env-file .env .output/server/index.mjs",
    "storyblok:download": "env-cmd -f .env.storyblok -x storyblok pull-components --space \\$STORYBLOK_SPACE_ID",
    "storyblok:generate": "node modules/cms/providers/storyblok/scripts/storyblok-generate.cjs && prettier --write modules/cms/providers/storyblok/types/storyblok.gen.d.ts",
    "storyblok:login": "env-cmd -f .env.storyblok -x storyblok login --token \\$STORYBLOK_PERSONAL_TOKEN --region eu",
    "storyblok:unused": "node modules/cms/providers/storyblok/scripts/storyblok-unused.cjs",
    "contentful:download": "env-cmd -f .env.contentful -x contentful-export --space-id \\$CONTENTFUL_SPACE_ID --management-token \\$CONTENTFUL_MANAGEMENT_TOKEN  --skip-webhooks --skip-roles --content-file contentful.\\$CONTENTFUL_SPACE_ID.json",
    "contentful:generate": "env-cmd -f .env.contentful -x cf-content-types-generator -X --response --typeguard ./contentful.\\$CONTENTFUL_SPACE_ID.json --out ./modules/cms/providers/contentful/types/gen && prettier --write modules/cms/providers/contentful/types/gen/** && find ./modules/cms/providers/contentful/types/gen -type f -name '*.ts' -print0 | xargs -0 sed -i '' -E \"s/Type([[:alnum:]]+)<'WITHOUT_(LINK_RESOLUTION|UNRESOLVABLE_LINKS)'>/Type\\1<'WITHOUT_\\2', LocaleCode>/g\"  ",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.8",
    "@eslint/eslintrc": "3.1.0",
    "@nuxt/eslint": "0.5.7",
    "@nuxt/image": "1.7.0",
    "@nuxt/test-utils": "3.14.2",
    "@nuxtjs/i18n": "8.5.3",
    "@nuxtjs/tailwindcss": "6.12.1",
    "@scayle/eslint-config-storefront": "4.3.0",
    "@scayle/eslint-plugin-vue-composable": "0.2.0",
    "@scayle/prettier-config-storefront": "2.0.2",
    "@types/color": "3.0.6",
    "@types/node": "20.16.5",
    "@typescript-eslint/scope-manager": "8.5.0",
    "@typescript-eslint/utils": "8.5.0",
    "@upstash/redis": "1.34.0",
    "@vitest/coverage-v8": "2.1.1",
    "@vue/test-utils": "2.4.6",
    "@zadigetvoltaire/nuxt-gtm": "0.0.13",
    "autoprefixer": "10.4.20",
    "env-cmd": "10.1.0",
    "eslint": "9.10.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-unimport": "0.0.3",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "happy-dom": "15.7.4",
    "lint-staged": "15.2.10",
    "nuxt": "3.11.2",
    "nuxt-svgo": "4.0.6",
    "ofetch": "1.3.4",
    "patch-package": "8.0.0",
    "pathe": "1.1.2",
    "postcss": "8.4.47",
    "postcss-custom-properties": "14.0.1",
    "postcss-html": "1.7.0",
    "postcss-import": "16.1.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.0.3",
    "storyblok-generate-ts": "2.1.0",
    "storyblok": "3.33.2",
    "tailwindcss": "3.4.11",
    "typescript": "5.6.2",
    "unimport": "3.12.0",
    "utility-types": "3.11.0",
    "vitest-environment-nuxt": "1.0.1",
    "vitest": "2.1.1",
    "vue-tsc": "2.1.6"
  },
  "dependencies": {
    "@contentful/live-preview": "4.5.6",
    "@contentful/rich-text-html-renderer": "16.6.10",
    "@googlemaps/js-api-loader": "1.16.8",
    "@scayle/nuxt-opentelemetry": "0.3.1",
    "@scayle/omnichannel-nuxt": "4.0.0",
    "@scayle/storefront-nuxt": "7.85.5",
    "@storyblok/nuxt": "6.0.10",
    "@storyblok/richtext": "2.0.0",
    "@storyblok/vue": "8.1.0",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@types/google.maps": "3.58.0",
    "@vercel/kv": "2.0.0",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "11.1.0",
    "@vueuse/nuxt": "11.1.0",
    "axios": "1.6.8",
    "cf-content-types-generator": "2.15.5",
    "check-password-strength": "2.0.10",
    "citty": "0.1.6",
    "color": "4.2.3",
    "consola": "3.2.3",
    "contentful": "10.15.1",
    "contentful-export": "7.19.148",
    "dotenv": "16.4.5",
    "global": "4.4.0",
    "globby": "14.0.2",
    "knitwork": "1.1.0",
    "maska": "2.1.11",
    "nanoid": "5.0.7",
    "nuxi": "3.13.2",
    "nuxt-jsonld": "2.0.8",
    "nuxt-swiper": "1.2.2",
    "redis": "4.7.0",
    "schema-dts": "1.1.2",
    "storyblok-js-client": "6.9.2",
    "ufo": "1.5.4",
    "vue": "3.4.38",
    "vue-slider-component": "4.1.0-beta.7"
  },
  "lint-staged": {
    "./**/*{.js,.mjs,.cjs,.ts,.vue}\"": [
      "prettier --write ",
      "eslint --fix "
    ],
    "./**/*{.md,.json,.css}\"": "prettier --write "
  },
  "volta": {
    "node": "20.17.0"
  },
  "resolutions": {
    "import-in-the-middle": "1.11.0",
    "@nuxt/kit": "3.11.2",
    "@nuxt/schema": "3.11.2",
    "vue": "3.4.38",
    "@vue/compiler-dom": "3.4.38",
    "@vue/compiler-sfc": "3.4.38",
    "@vue/compiler-core": "3.4.38",
    "@vue/compiler-ssr": "3.4.38",
    "@vue/shared": "3.4.38"
  }
}
